{
  "name": "sfaxas",
  "version": "0.0.1",
  "description": "Siulüsemüsifaxas, SFaxas는 인공 세계관 사트의 보드게임들을 플레이 할 수 있는 사이트입니다.",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "concurrently \"npm:*:watch\"",
    "node:watch": "nodemon . -e js,json,html,ejs,css --ignore \"./styles/style.css ./public/style.css\"",
    "tw:watch": "tailwindcss -i ./styles/style.css -o ./public/style.css -w",
    "build": "tailwindcss -i ./styles/style.css -o ./public/style.css/"
  },
  "repository": "git+https://github.com/cedille789/sfaxas.git",
  "keywords": [
    "sfaxas",
    "siulüsemüsifaxas",
    "sat",
    "board",
    "game",
    "game",
    "online",
    "multiplayer",
    "website"
  ],
  "author": "Sediü",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cedille789/sfaxas/issues"
  },
  "homepage": "https://github.com/cedille789/sfaxas#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "flowbite": "^1.6.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
